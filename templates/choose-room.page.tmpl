{{template "base" .}}

{{define "content"}}

    <div class="container">
        <div class="row">
            <div class="col">
                <h1 class="text-center my-4">Choose a room</h1>

                {{$rooms := index .Data "rooms"}}
                {{range $rooms}}
                    <div>{{.RoomName}}</div>
                {{end}}
            </div>
        </div>
    </div>

{{end}}